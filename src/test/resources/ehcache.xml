<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd"
         name="CM1"
         updateCheck="false"
         maxBytesLocalHeap="16M">

    <diskStore path="java.io.tmpdir"/>

    <defaultCache
            eternal="false"
            overflowToDisk="false"
            />

    <cache name="edu.codespring.profinet.domain.User"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="edu.codespring.profinet.domain.Authority"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="edu.codespring.profinet.domain.User.authorities"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="edu.codespring.profinet.domain.PersistentToken"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="edu.codespring.profinet.domain.User.persistentTokens"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="edu.codespring.profinet.domain.PersistentAuditEvent"
           timeToLiveSeconds="3600">
    </cache>
    
    <cache name="edu.codespring.profinet.domain.Expert" timeToLiveSeconds="3600"/>
    <cache name="edu.codespring.profinet.domain.Keyword" timeToLiveSeconds="3600"/>
    <cache name="edu.codespring.profinet.domain.Field" timeToLiveSeconds="3600"/>
    <cache name="edu.codespring.profinet.domain.UserFeedback" timeToLiveSeconds="3600"/>
    <cache name="edu.codespring.profinet.domain.Message" timeToLiveSeconds="3600"/>
    <cache name="edu.codespring.profinet.domain.Language" timeToLiveSeconds="3600"/>
    <cache name="edu.codespring.profinet.domain.ExpertContactRequest" timeToLiveSeconds="3600"/>
    <cache name="edu.codespring.profinet.domain.Appointment" timeToLiveSeconds="3600"/>
    <cache name="edu.codespring.profinet.domain.Expert.expertfields" timeToLiveSeconds="3600"/>
    <cache name="edu.codespring.profinet.domain.Expert.expertkeywords" timeToLiveSeconds="3600"/>
    <cache name="edu.codespring.profinet.domain.Expert.expertlanguages" timeToLiveSeconds="3600"/>
    
</ehcache>

